<odoo>
  <data>
    <record model="ir.module.category" id="module_management">
      <field name="name">Programa de obra</field>
      <field name="description">User access level for this module</field>
      <field name="sequence">9</field>
    </record>

    <record id="group_administrador" model="res.groups">
     <field name="name">Administrador</field>
     <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
     <field name="category_id" ref="ejecucion_obra.module_management"/>
    </record>

    <record id="group_coordinador" model="res.groups">
      <field name="name">Coordinador</field>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
      <field name="category_id" ref="ejecucion_obra.module_management"/>
    </record>
    
    <record id="group_usuario" model="res.groups">
      <field name="name">Usuario</field>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
      <field name="category_id" ref="ejecucion_obra.module_management"/>
    </record>

    <record id="group_consulta" model="res.groups">
      <field name="name">Consulta</field>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
      <field name="category_id" ref="ejecucion_obra.module_management"/>
    </record>

 </data>
</odoo>
