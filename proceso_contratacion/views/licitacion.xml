    <!-- LICITACIONES TREE -->
    <record model="ir.ui.view" id="proc_contratacion_licitacion_tree">
      <field name="name">proceso licitacion tree</field>
      <field name="model">proceso.licitacion</field>
      <field name="arch" type="xml">
        <tree decoration-muted="estado_obra_desierta, '>=', '1'" decoration-danger="estado_obra_cancelar, '>=', '1'" default_order="create_date desc">
          <field name="estado_obra_desierta" invisible="1"/>
          <field name="estado_obra_cancelar" invisible="1"/>
          <field name="tipolicitacion" string="Tipo"/>
            <field name="numerolicitacion" string="Licitacion"/>
          <field name="convocatoria" string="Conv."/>
          <field name="name" string="Descripcion"/>
          <field name="normatividad"/>
          <field name="fechaconinv" string="Fecha"/>
          <field name="estatus" string="Estatus"/>
          <field name="create_date" string="Fecha de Reg"/>
        </tree>
      </field>
    </record>

<!-- LICITACION SEARCH -->
  <record model="ir.ui.view" id="proceso_licitacion_search">
         <field name="name">Contratacion Licitacion</field>
