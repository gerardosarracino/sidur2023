              <button type="action" name="%(registro_obras.seguimientoobra_action)d" icon="fa-calendar" context="{
                       'search_default_name': active_id,
                       'default_name': active_id
                       }" class="btn btn-outline-primary btn-lg">
                <field string="Seg. Obra" name="seguimientoc" widget="statinfo"/></button>

              <button type="action" name="%(registro_obras.programarobra_action)d" icon="fa-clock-o" context="{
                       'search_default_name': active_id,
                       'default_name': active_id,
                       'default_obra_planeada': active_id
                       }" attrs="{'invisible': [('estado_obra', '=', 'Programada')]}"
              class="btn btn-outline-primary btn-lg">

                <field string="Progr. Obra" name="programada" widget="statinfo"/></button>
          	</div>

            <div class="oe_title">
                            <label for="numero_obra" class="oe_edit_only"/>
                            <h1>
                                <field name="numero_obra" placeholder="Número de obra"/>
                            </h1>
                        </div>
            <field name="id_sideop" invisible="1"/>
            <field name="name" invisible="1"/>
            <field name="programada_sideop" invisible="1"/>

            <group>

            <field name="estado_obra" invisible="1"/>
            <field name="estate" invisible="1"/>
            </group>

            <group string="Datos generales de la obra" colspan="1" col="4">
              <field name="ejercicio" class="oe_inline" options="{'no_create': True, 'no_create_edit':True, 'not_delete': True}" 
              domain="[('name', '>', '2019')]" />
              <field name="grupoObra"/>
              <field name="origen" string="Origen" class="oe_inline" options="{'no_create': True, 'no_create_edit':True, 'not_delete': True}"/>
              <field name="monto" class="oe_inline"/>
            </group>
            <group>
              <field name="descripcion"/>
              <field name="problematica"/>
            </group>
            <group colspan="1" col="4">
              <field name="unidadadminsol" options="{'no_create': True, 'no_create_edit':True, 'not_delete': True}"/>
              <field name="tipoObra" string="Tipo de obra" options="{'no_create': True, 'no_create_edit':True, 'not_delete': True}"/>
              <field name="tipoproyecto" options="{'no_create': True, 'no_create_edit':True, 'not_delete': True}"/>
              <field name="tipoobraetapa" string="Tipo de obra etapa" options="{'no_create': True, 'no_create_edit':True, 'not_delete': True}"/>
            </group>

            <group string="Impacto de la obra" colspan="2" col="4">
              <field name="beneficiados"/>
