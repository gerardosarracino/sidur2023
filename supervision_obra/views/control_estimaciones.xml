      <record id="supervision_obra_detalle_s" model="ir.ui.view">
          <field name="name">detalle conceptos search</field>
          <field name="model">control.detalle_conceptos</field>
          <field name="arch" type="xml">
              <search string="">
                  <filter name="numero_estimacion_group" string="Numero de Estimacion" context="{'group_by':'numero_estimacion_group'}"/>
                  <field name="num_est"/>
                  <field name="categoria"/>
                  <field name="descripcion"/>
                  <field name="clave_linea"/>
                  <field name="concepto"/>
                  <field name="cantidad"/>
                  <field name="estimacion"/>
                  <field name="id_partida"/>
                  <field name="numero_estimacion_group"/>
              </search>
          </field>
      </record>


      <record id="supervision_detalle_tree" model="ir.ui.view">
          <field name="name">detalle import</field>
          <field name="model">control.detalle_conceptos</field>
          <field name="arch" type="xml">
              <tree string="" create="false" delete="false"
                                    decoration-info="precio_unitario == 0"
                            decoration-bf="precio_unitario == 0">
                  <field name="id_partida" invisible="1"/>
                  <field name="num_est" string="# Est" invisible="1" />
                  <field name="numero_estimacion_group" string="# Est" />
                  <field name="clave_linea" readonly="1" string="Clave"/>

                    <field name="related_categoria_padre" string="Grupo" domain="[('id_partida.id', '=', id_partida)]"/>

                    <field name="categoria"  force_save="1" domain="[('id_partida.id', '=', id_partida)]" invisible="1"/>

                    <field name="concepto" readonly="1" string="Concepto"
                    attrs="{'invisible': ['|', ('concepto','=', '-'),('concepto','=', '.')]}"/>

                    <field name="medida" readonly="1" string="U.M" force_save="1"
                           attrs="{'invisible': ['|', ('medida','=', '-'),('medida','=', '.')]}" />
                    <field name="precio_unitario" readonly="1" attrs="{'invisible':[('precio_unitario','=',0.0)]}" />
                    <field name="cantidad" readonly="1" attrs="{'invisible':[('cantidad','=',0)]}"
                    string="Contratada"/>

                    <field name="est_ant" attrs="{'invisible':[('est_ant','=',0.0)]}" readonly="1" invisible="1"/>
                    <field name="pendiente" attrs="{'invisible':[('pendiente','=',0.0)]}" readonly="1" invisible="1"/>
                    <field name="estimacion" attrs="{'readonly':[('precio_unitario','=',0.0)], 'invisible':[('precio_unitario','=',0.0)]}"/>
                    <field name="importe_ejecutado" sum="Importe Ejecutado"
                           attrs="{'invisible':[('importe_ejecutado','=',0.0)]}" readonly="1"/>
              </tree>
          </field>
      </record>

