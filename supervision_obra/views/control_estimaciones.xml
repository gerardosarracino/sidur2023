<odoo>
  <data>

      <record id="supervision_obra_conceptos_search_est" model="ir.ui.view">
         <field name="name">concepto search</field>
         <field name="model">proceso.conceptos_part</field>
          <field name="arch" type="xml">
              <search string="">
                <field name="id_partida" />
                <field name="name" />
                <field name="descripcion" />
                <field name="categoria" />
                <field name="clave_linea"/>
                <field name="concepto"/>
                <field name="medida"/>
                <field name="cantidad" />
                <field name="precio_unitario" />
                <field name="importe"/>
          </search>
         </field>
      </record>

      <!-- REPORTERIA -->
       <report
            id="estimacion_report"
            model="control.estimaciones"
            string="Control de Estimaciones"
            report_type="qweb-html"
            name="report_estimaciones.report_estimaciones"
        />

      <report
            id="estimacion_report_relacion_con"
            model="control.estimaciones"
            string="Relación de Conceptos"
            report_type="qweb-html"
            name="report_estimaciones.report_conceptos"
        />

      <report
            id="estimacion_report_penas"
            model="control.estimaciones"
            string="Penas Convencionales"
            report_type="qweb-html"
            name="report_estimaciones.report_penas_con"
        />

      <report
            id="estimacion_report_concentrado"
            model="control.estimaciones"
            string="Estimación Concentrado"
            report_type="qweb-html"
            name="report_estimaciones.report_concentrado"
        />

    <!-- ESTIMACIONES FORM -->
  <record id="supervision_obra_estimaciones_form" model="ir.ui.view">
        <field name="name">control de estimaciones form</field>
        <field name="model">control.estimaciones</field>
        <field name="arch" type="xml">
            <form string="" edit="false" create="false" delete="false">
                <header>
                     <button string="Conceptos Estimacion Excel" name="imprimir_accion_excel" type="object"
                                icon="fa-book" class="btn btn-outline-success"/>

                     <button name="%(estimacion_report)d" string="Control de Estimaciones" type="action"  icon="fa-copy" />

                     <button name="%(estimacion_report_relacion_con)d" string="Relacion de Conceptos" type="action"  icon="fa-copy" />

                     <button name="%(estimacion_report_penas)d" string="Penas Convencionales" type="action"  icon="fa-copy" />

                     <button name="%(estimacion_report_concentrado)d" string="Estimacion Concentrado" type="action"  icon="fa-copy" />

                </header>

                <sheet>

                    <div>
                        <h1>
                            <field name="ide_estimacion" readonly="1" force_save="1"/>
                        </h1>
                    </div>

                    <notebook>
                        <page string="Datos Generales De La Estimación">


                            <group string="Datos Generales De La Estimación" colspan="1" col="2">
                                <group>
                                    <field name="estimacion_ids" readonly="1" force_save="1" invisible="1" />
                                    <!-- por mientras para activar metodos -->
                                    <field name="p_id" invisible="1" />
                                    <field name="obra"/>

                                    <field name="retenido_anteriormente" readonly="1" force_save="1" invisible="1"/>
                                    <field name="a_fin"/>
                                    <field name="clave_obra" force_save="1" invisible="1"/>
                                    <field name="clave_estimacion" invisible="1"/>
                                    <br/>
                                    <field name="tipo_estimacion" widget="radio"/>
                                </group>
                                <group>
                                    <field name="idobra" readonly="1" class="oe_inline" style="font-size: 13px;"
                                    force_save="1"/>

                                    <hr attrs="{'invisible': [('tipo_estimacion','!=', '2')]}"/>

                                    <field name="estimacion_esc" attrs="{'invisible': [('tipo_estimacion','!=', '2')],
                                    'required': [('tipo_estimacion','=', '2')]}"
                                     domain="[('obra.id', '=', obra)]"/>
                                    <hr attrs="{'invisible': [('tipo_estimacion','!=', '2')]}"/>
                                    <field name="por_escalatoria" attrs="{'invisible': [('tipo_estimacion','!=', '2')],
                                    'required': [('tipo_estimacion','=', '2')]}"
                                    class="oe_inline" />

                                    <hr attrs="{'invisible': [('tipo_estimacion','!=', '2')]}"/>

                                    <field name="sub_total_esc" attrs="{'invisible': [('tipo_estimacion','!=', '2')]}"
                                    class="oe_inline"/>

                                    <hr attrs="{'invisible': [('tipo_estimacion','!=', '2')]}"/>
                                    <field name="sub_total_esc_h" attrs="{'invisible': [('tipo_estimacion','!=', '2')]}"
                                    class="oe_inline" force_save="1" readonly="1" string="Importe"/>

                                </group>
                            </group>

                            <group>
                                <field name="multiples_escalatorias" widget="one2many" attrs="{'invisible': [('tipo_estimacion','!=', '3')]}" nolabel="1"
                                context="{
                                       'default_obra': obra,
                                       }">
                                        <tree editable="bottom">
                                            <field name="estimacion" domain="[('obra.id', '=', obra)]"/>
                                            <field name="fecha_inicio_estimacion" force_save="1"/>
                                            <field name="fecha_termino_estimacion" force_save="1"/>
                                            <field name="idobra" readonly="1" force_save="1"/>
                                            <field name="obra" force_save="1" readonly="1" invisible="1"/>
                                            <field name="por_escalatoria"/>
                                            <field name="sub_total_esc"/>
                                            <field name="sub_total_esc_h" string="Importe" readonly="1" force_save="1"/>
                                        </tree>
                                    </field>
                            </group>
                            <group col="6">
                                <p>&#160;</p>
                                <p>&#160;</p>
                                <p>&#160;</p>
                                <p>&#160;</p>
                                <field name="sub_total_esc_h" attrs="{'invisible': [('tipo_estimacion','!=', '3')]}"
                                    class="oe_inline" force_save="1" readonly="1" string="Importe" style="float:right;"/>
                            </group>

                            <group  colspan="2" col="2">
                                <group string="Periodo de la Estimación">
                                    <field name="fecha_inicio_estimacion" class="oe_inline"/>
                                    <br/>
                                    <field name="fecha_termino_estimacion" class="oe_inline"/>
                                </group>
                                <group string="Seguimiento">
                                    <field name="fecha_presentacion" class="oe_inline"/>
                                    <br/>
                                    <field name="fecha_revision" class="oe_inline"/>
                                </group>
                                <group string="Indique si Aplica">
                                    <field name="si_aplica" widget="radio" nolabel="1"/>
                                </group>
                                <group string="Notas:">
                                    <field name="notas" nolabel="1"/>
                                </group>
                            </group>

                                <group colspan="10" col="1" string="Deducciones">
                                    <field name="deducciones" nolabel="1" force_save="1">
                                        <tree create="false" force_save="1" delete="false">
                                            <!-- field name="estimacion" force_save="1"/ -->
                                            <field name="name" string="Deducción"/>
                                            <field name="porcentaje" force_save="1" />
                                            <field name="valor" force_save="1" />
                                        </tree>
                                    </field>
                                </group>


                                <sheet>
                                    <br/>
                                    <group string="Cálculo De Estimación"  colspan="2" col="3">
                                        <group>
                                            <p colspan="4" align="left">Importe ejecutado estimación:   $<field name="estimado" nolabel="1" readonly="1"
                                                                                                                force_save="1"
                                            style="color: #237570; font-size: 17px;"/></p>
                                        </group>
                                        <group>
                                            <p colspan="4" align="left">Amortización de Anticipo <field name="amort_anticipo_partida" placeholder="Ingresar % de anticipo"
                                                                                                        force_save="1"

                                            style="color: #048A72;" class="oe_inline"/>%:   $<field name="amort_anticipo" nolabel="1" force_save="1"
                                            style="color: #237570;font-size: 17px;"/></p>
                                         </group>
                                        <group>
                                            <p colspan="4" align="left">Neto Estimación sin IVA: $<field name="estimacion_subtotal" nolabel="1"
                                                                                                         readonly="1" force_save="1"
                                            style="color: #237570;font-size: 17px;"/></p>
                                        </group>
                                    </group>

                                    <group  colspan="2" col="3">
                                        <group>
                                            <p colspan="4" align="left">I.V.A. 16%: $<field name="estimacion_iva" force_save="1"
                                                                                            readonly="1"
                                            style="color: #237570;font-size: 17px;"/></p>
                                        </group>
                                        <group>
                                            <p colspan="4" align="left">Neto Estimación con IVA: $<field name="estimacion_facturado" force_save="1"
                                                                                                         readonly="1"
                                            style="color: #237570;font-size: 17px;"/></p>
                                         </group>
                                        <group>
                                            <p colspan="4" align="left">Menos Suma Deducciones: $<field name="estimado_deducciones" force_save="1"
                                                                                                        readonly="1"
                                            style="color: #237570;font-size: 17px;"/></p>
                                        </group>
                                    </group>

                                    <group  colspan="2" col="3">
                                        <group>
                                            <p colspan="4" align="left">Retención/Devolución: $<field name="ret_neta_est" nolabel="1"
                                                                                                      readonly="1" force_save="1"
                                            style="color: #237570;font-size: 17px;"/></p>
                                        </group>
                                        <group>
                                            <p colspan="4" align="left">	Sanción por Incump. de plazo: $<field name="sancion" nolabel="1" force_save="1"
                                                                                                                  readonly="1"
                                            style="color: #237570;font-size: 17px;"/></p>
                                         </group>
                                        <group>
                                            <p colspan="4" align="left">	Importe liquido: $<field name="a_pagar" nolabel="1" force_save="1"
                                                                                                     readonly="1"
                                            style="color: #237570;font-size: 17px;"/></p>
                                        </group>
                                    </group>
                              </sheet>


                                    <group string="Penas Convencionales" colspan="2" col="4">
                                            <field name="menos_clau_retraso" class="oe_inline"/>
                                            <field name="sancion_incump_plazo" class="oe_inline"/>
                                    </group>


                                </page>

                        <page string="Conceptos Ejecutados En El Periodo"  attrs="{'invisible': [('tipo_estimacion','!=', '1')]}" > <!-- attrs="{'invisible': [('tipo_estimacion','!=', '1')]}" -->
                            <separator string="Conceptos Ejecutados En El Periodo"/>
                            <group col="3">
                                <field name="estimado" string="TOTAL DE CONCEPTOS: " readonly="1" force_save="1"/>

                                <button name="actualizar_conceptos" type="object" string="ACTUALIZAR CONCEPTOS"
                                                   class="btn btn-outline-success btn-lg" style="float:right;"/>
                            </group>
                            <button name="ver_conceptos" type="object" string="VER CONCEPTOS"
                                           class="btn btn-outline-primary btn-lg"/>

                            <field name="dias_transcurridos"  force_save="1" />
                            <field name="diasdif"  force_save="1" />
                            <field name="diasrealesrelacion"  force_save="1" />
                            <field name="dias_desfasamiento"  force_save="1" />

                            <field name="conceptos_partidas" string="" widget="one2many" force_save="1" context="{'form_view_ref': 'ejecucion_obra.supervision_obra_conceptos_estimados_form'}">
                                    <tree editable="bottom" delete="false" create="false" force_save="1"
                                    decoration-info="precio_unitario == 0"
                            decoration-bf="precio_unitario == 0"
                            >
                                        <field name="num_est" force_save="1"  invisible="1"/>
                                        <field name="id_partida"  force_save="1" readonly="1" invisible="1"/>

                                        <field name="clave_linea" readonly="1" string="Clave" force_save="1"/>

                                        <field name="related_categoria_padre"  force_save="1" string="Grupo" domain="[('id_partida.id', '=', id_partida)]"/>

                                        <field name="categoria"  force_save="1"  class="text-left" domain="[('id_partida.id', '=', id_partida)]" readonly="1"
                                        />

                                        <field name="concepto" readonly="1" string="Concepto" force_save="1"
                                        attrs="{'invisible': ['|', ('concepto','=', '-'),('concepto','=', '.')]}"/>

                                        <field name="medida" readonly="1" string="U.M" force_save="1"
                                               attrs="{'invisible': ['|', ('medida','=', '-'),('medida','=', '.')]}"/>
                                        <field name="precio_unitario" readonly="1" attrs="{'invisible':[('precio_unitario','=',0.0)]}"
                                        force_save="1" />
                                        <field name="cantidad" readonly="1" attrs="{'invisible':[('cantidad','=',0)]}"
                                        string="Contratada" force_save="1"/>

                                        <field name="est_ant" attrs="{'invisible':[('est_ant','=',0.0)]}" readonly="1"/>
                                        <field name="pendiente" attrs="{'invisible':[('pendiente','=',0.0)]}" readonly="1"/>
                                        <field name="estimacion" force_save="1" attrs="{'readonly':[('precio_unitario','=',0.0)], 'invisible':[('precio_unitario','=',0.0)]}"/>
                                        <field name="importe_ejecutado" sum="Importe Ejecutado"
                                               attrs="{'invisible':[('importe_ejecutado','=',0.0)]}" readonly="1"/>

                                    </tree>
                                </field>

                            <group col="4">
                                <field name="fecha_termino_programa"  force_save="1" invisible="1"/>
                                    <field name="fecha_inicio_programa"  force_save="1" invisible="1" />
                                    <field name="nuevo_metodo"  force_save="1" invisible="1" />
                                    <field name="monto_programado_est"  force_save="1" invisible="1"/>
                                    <field name="dias_atraso_sancion"  force_save="1" invisible="1" />
                                    <field name="retenido_anteriormente"  force_save="1" invisible="1" />
                                    <field name="montoreal"  force_save="1" invisible="1" />
                                    <field name="diasdif"  force_save="1" invisible="1"/>
                                    <field name="dias_desfasamiento"  force_save="1" invisible="1"/>
                                    <field name="monto_atraso"  force_save="1" invisible="1" />
                                    <field name="diasperiodo"  force_save="1" invisible="1"/>
                                    <field name="diasest"  force_save="1" invisible="1"/>
                                    <field name="diastransest"  force_save="1" invisible="1"/>
                                    <field name="total_ret_est"  force_save="1" invisible="1"/>
                                    <field name="porc_total_ret"  force_save="1" invisible="1"/>
                                    <field name="porcentaje_est"  force_save="1" invisible="1"/>
                                    <field name="ret_neta_est"  force_save="1" invisible="1"/>
                                    <field name="devolucion_est"  force_save="1" invisible="1"/>
                                    <field name="montodiario_programado"  force_save="1" invisible="1"/>
                                    <field name="diasrealesrelacion"  force_save="1" invisible="1"/>
                                    <field name="estimacion_subtotal"  force_save="1" invisible="1"/>
                                    <field name="dias_transcurridos"  force_save="1" invisible="1" />
                                    <field name="b_iva"  force_save="1" invisible="1"/>
                                    <field name="sub_total_esc_h"  force_save="1" invisible="1"/>
                                    <field name="ultimomonto"  force_save="1" invisible="1" />
                            </group>
                            <field name="total_conceptos" readonly="1" invisible="1" />
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

      <!-- PARTIDAS ESTIMACIONES SEARCH-->
      <record model="ir.ui.view" id="supervision_obra_estimaciones_search">
         <field name="name">elaboracion contrato</field>
         <field name="model">control.estimaciones</field>
         <field name="arch" type="xml">
            <search>
                <field name="obra"/>
                <field name="ide_estimacion"/>
                <field name="tipo_estimacion"/>
                <field name="fecha_inicio_estimacion" />
                <field name="fecha_termino_estimacion"/>
                <field name="estimado"/>
                <field name="amort_anticipo"/>
                <field name="estimacion_subtotal"/>
                <field name="estimacion_facturado"/>
                <field name="estimado_deducciones"/>
                <field name="total_ret_est"/>
                <field name="sancion"/>
                <field name="a_pagar"/>
            </search>
         </field>
        </record>

    <!-- ESTIMACIONES TREE -->
    <record id="supervision_obra_supervision_obra_control_estimaciones_tree" model="ir.ui.view">
        <field name="name">control de estimaciones tree</field>
        <field name="model">control.estimaciones</field>
        <field name="arch" type="xml">
            <tree string="" decoration-info="tipo_estimacion=='2'" create="false" delete="false">
                <field name="idobra" string="#"/>
                <field name="obra" invisible="1"/>
                <field name="ide_estimacion" string="# Id"/>
                <field name="tipo_estimacion" string="Tipo"/>
                <field name="fecha_inicio_estimacion" string="Fecha Inicio" />
                <field name="fecha_termino_estimacion" string="Fecha Termino"/>
                <field name="estimado" string="Estimado" sum="Estimado"/>
                <field name="amort_anticipo" string="Amort. anticipo" sum="Total Amort."/>
                <field name="estimacion_subtotal" string="Subtotal" sum="Total Subtotal"/>
                <field name="estimacion_iva" string="I.V.A" sum="Total I.V.A"/>
                <field name="estimacion_facturado" string="Facturado" sum="Total Facturado"/>
                <field name="estimado_deducciones" string="Deducciones" sum="Total Deducciones"/>

                <field name="ret_neta_est" string="Ret/Dev." sum="Total Ret/Dev"/>

                <field name="sancion" string="Sanción" sum="Total Sanción"/>

                <field name="a_pagar" string="A pagar" sum="Total Importe Liquido"/>

            </tree>
        </field>
    </record>

 <!-- BOTON ESTIMACIONES -->
<!--record model="ir.actions.act_window" id="supervision_obra_action_estimaciones1">
        <field name="name">Estimaciones</field>
        <field name="res_model">control.estimaciones</field>
        <field name="type">ir.actions.act_window</field>
