<record model="ir.actions.act_window" id="supervision_obra_action_estimaciones">
        <field name="name">Estimaciones</field>
        <field name="res_model">control.estimaciones</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_ids" eval="[(5, 0, 0),
    (0, 0, {'view_mode': 'tree', 'view_id': ref('supervision_obra_supervision_obra_control_estimaciones_tree')}),
    (0, 0, {'view_mode': 'form', 'view_id': ref('supervision_obra_estimaciones_form')})]"/>
    </record>

      <!-- VISTA TREE DE PARTIDAS -->
      <record model="ir.ui.view" id="supervision_obra_control_estimaciones_tree">
      <field name="name">Estimaciones Tree</field>
      <field name="model">partidas.partidas</field>
      <field name="arch" type="xml">
        <tree create="false" delete="false" default_order="obra desc">
           <field name="fechainicio"/>
            <field name="fechatermino"/>
            <field name="nombre_partida" string="Subcontrato"/>
            <field name="tipo_obra" />
            <field name="objeto"/>
            <field name="contratista" />
            <field name="obra"/>
            <field name="total" string="Importe Contratado" />
             <field name="a_fis" string="A. fis"/>
            <field name="porcentajeProgramado" string="P. prog" />
            <field name="a_fin" />
            <field name="contar_estimaciones" string="# de ESt."/>
        </tree>
      </field>
    </record>


      <record model="ir.actions.act_window" id="conceptos_tree_action2">
      <field name="name">Conceptos</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">proceso.conceptos_part</field>
    <field name="context">{'search_default_group_categoria': True}</field>
      <field name="view_mode">tree,pivot</field>
    </record>


      <!-- VISTA CON BOTONES HACIA ESTIMACION -->
    <record id="supervision_obra_vista_form_estimaciones" model="ir.ui.view">
        <field name="name">vista form estimaciones</field>
        <field name="model">partidas.partidas</field>
        <field name="arch" type="xml">
            <form string="" edit="false" create="false" delete="false">
                <header>
                     <button string="Total Conceptos Estimados" name="imprimir_accion_excel" type="object"
                                icon="fa-book" class="btn btn-outline-success"/>
                    </header>
                <sheet>

                <div class="text-right" attrs="{'invisible': [('verif_programa', '=', False)]}">

                    <button name="ProgramasVentana" class="btn btn-outline-primary btn-lg " type="object"
                        string="PROGRAMA DE LA OBRA" context="{'default_obra': active_id, 'default_obraid': id}"
                        icon="fa-clock-o"/>

                     <button string="CONCEPTOS" type="action" name="%(conceptos_tree_action2)d" class="btn btn-outline-primary btn-lg" context="{
                       'search_default_id_partida': active_id}" icon="fa-book"/>

                    <button type="object" name="conceptos_estimados_tree" class="btn btn-outline-primary btn-lg"
                          string="CONCEPTOS ESTIMADOS " context="{
                          'search_default_id_partida': active_id,
                       'search_default_group_numero_estimacion_group': 1}"  icon="fa-indent"
                  />

                      <button type="action" name="%(supervision_obra_action_estimaciones)d" class="btn btn-outline-primary btn-lg"
                          string="ESTIMACIONES " context="{
                           'search_default_obra': active_id,
                           'default_obra': active_id
                           }"  icon="fa-usd">
                          <field name="contar_estimaciones" widget="statinfo" string="Estimaciones"/>
                  </button>
                      <!-- <button type="action" icon="fa-list" string="Estado de Cuenta"></button>  <field name="context">{'search_default_group_nombre_prog': True}</field> -->
                </div>


                <notebook>
                <page string="Datos Generales de la Partida">
                    <field name="contar_estimaciones" force_save="1" invisible="1"/>
                    <!-- dejar partida id la uso para activar un metodo por mientras-->
                    <field name="p_id" invisible="1"/>

                    <field name="verif_programa" readonly="1" invisible="1"/>
                    <field name="odoo_user_admin" invisible="1" force_save="1"/>

                   <group  colspan="2" col="4">
                            <div>
                            <b>Nombre de la Obra: </b>
                        </div>
                        <p>&#160;</p>
                         <p attrs="{'invisible': [('verif_programa', '=', True)]}"
                            align="right" style="color: #DE0C0C">No hay Programa Para La Estimación*</p>
                   </group>
                    <group>
                        <field name="obra" nolabel="1" readonly="1"/>
                    </group>
                    <group><div>
                            <b>Contratista: </b>
                        </div></group>
                    <group>
                        <field name="contratista" nolabel="1" readonly="1"/>
                    </group>

                            <hr/>
                        <group colspan="2" col="5" context="{'default_ejercicio': obra.id}">
                            <b>Programa</b>
                            <b>Ejercicio</b>
                            <b>Monto</b>
                            <b>Municipio</b>
                            <b>Localidad</b>
                        </group>
                        <group colspan="2" col="6">
                            <field name="programaInversion" readonly="True" string="" nolabel="1"/>
                            <field name="ejercicio" string="" nolabel="1"/>
                            <field name="total_partida" widget="monetary" string="" nolabel="1"/>
                            <field name="municipio" string="" nolabel="1"/>
                            <field name="localidad" string="" nolabel="1"/>
                        </group>
                          <hr/>
                        <group colspan="2" col="5">
                            <b>Fecha del Anticipo</b>
                            <b>Fecha del contrato</b>
                            <b>Fecha de Inicio Contractual</b>
                            <b>Fecha de Termino Contractual</b>
                            <b>Supervision Externa</b>
                        </group>

                        <group colspan="2" col="6">
                          <field name="fecha_anticipos" readonly="1" string="" nolabel="1"/>
                          <field name="fecha" string="" nolabel="1"/>
                          <field name="fechainicio" string="" nolabel="1"/>
                          <field name="fechatermino" string="" nolabel="1"/>
                          <field name="supervisionexterna1" string="" nolabel="1" options="{'no_create': True, 'no_create_edit':True, 'not_delete': True}"/>
